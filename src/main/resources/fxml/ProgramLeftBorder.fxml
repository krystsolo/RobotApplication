<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RobotApplication.controllers.ProgramLeftBorderController">
   <children>
      <Label contentDisplay="CENTER" prefHeight="20.0" prefWidth="120.0" text="Program">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="15.0" />
         </padding>
      </Label>
      <Label fx:id="programNameLabel" prefWidth="120.0" textFill="#2402f7">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="30.0" />
         </padding>
      </Label>
      <Button fx:id="loadProgramButton" mnemonicParsing="false" onAction="#loadProgramOnAction" prefHeight="40.0" prefWidth="120.0" text="Wczytaj" />
      <Button fx:id="saveProgramButton" mnemonicParsing="false" onAction="#saveProgramOnAction" prefHeight="40.0" prefWidth="120.0" text="Zapisz" />
      <Button fx:id="saveProgramAsButton" mnemonicParsing="false" onAction="#saveProgramAsOnAction" prefHeight="40.0" prefWidth="120.0" text="Zapisz jako" />
      <Button fx:id="createProgramButton" mnemonicParsing="false" onAction="#createProgramOnAction" prefHeight="40.0" prefWidth="120.0" text="Utwórz nowy" />
      <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteProgramOnAction" prefHeight="40.0" prefWidth="120.0" text="Usuń" />
      <Button fx:id="nameChangeButton" mnemonicParsing="false" onAction="#changeProgramNameOnAction" prefHeight="40.0" prefWidth="120.0" text="Zmień nazwę" />
      <Button fx:id="clearListViewButton" mnemonicParsing="false" onAction="#clearListViewOnAction" prefHeight="40.0" prefWidth="120.0" text="Zamknij program" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
